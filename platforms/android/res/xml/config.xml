<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.stations" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hello Cordova</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <name>Stations</name>
    <description>
        Stations
    </description>
    <author email="njhamann@gmail.com" href="http://noahhamann.com">
        Noah Hamann
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="AllowInlineMediaPlayback" value="true" />
    <feature name="Chromecast">
        <param name="android-package" value="org.apache.cordova.plugin.chromecast" />
    </feature>
</widget>
